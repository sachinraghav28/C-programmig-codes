#include<stdio.h>
    int main(){
        int row,col,a,b,sum,i,j;
        scanf("%d%d",&row,&col);
        for(i=0;i<row;i++){
            for(j=0;j<col;j++){
            scanf("%d",&a[i][j]);
           }
        }
        for(i=0;i<row;i++){
            for(j=0;j<col;j++){
            scanf("%d",&b[i][j]);

            }
        }
        for(i=0;i<row;i++){
            for(j=0;j<col;j++){
            sum[i][j]=a[i][j]+b[i][j];
            printf("%d\t",sum[i][j]);
        }

        printf("\n");
    return 0;
    }
